{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "flowtype"
  ],
  "globals": {
    "fetch": true,
    "navigator": true
  },
  "rules": {
    "class-methods-use-this": 0,
    "no-use-before-define": 0,
    "no-console": 0,
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "react/sort-comp": 0,
    "flowtype/define-flow-type": 1,
    "flowtype/use-flow-type": 1
  }
}
